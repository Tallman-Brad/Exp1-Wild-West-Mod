<wildwestmod>

	<remove xpath="/rwgmixer/township[@name='forest_countrytown']"/>
	<remove xpath="/rwgmixer/township[@name='bforest_countrytown']"/>
	<remove xpath="/rwgmixer/township[@name='bforest_town']"/>
	<remove xpath="/rwgmixer/township[@name='countrytown']"/>	
	<remove xpath="/rwgmixer/township[@name='town']"/>
	<remove xpath="/rwgmixer/township[@name='city']"/>
	<remove xpath="/rwgmixer/township[@name='wasteland_city']"/>
	<remove xpath="/rwgmixer/township[@name='citybig']/property[@name='outskirt_district']"/>
	<!--remove xpath="/rwgmixer/township[@name='roadside']"/-->
	<!--remove xpath="/rwgmixer/district[@name='wilderness']"/-->
	<remove xpath="/rwgmixer/district[@name='countrytown']"/>
	<remove xpath="/rwgmixer/district[@name='countryresidential']"/>
	<remove xpath="/rwgmixer/district[@name='commercial']"/>
	<remove xpath="/rwgmixer/district[@name='industrial']"/>
	<remove xpath="/rwgmixer/district[@name='residential']"/>
	<!--remove xpath="/rwgmixer/district[@name='rural']"/-->	
	<remove xpath="/rwgmixer/district[@name='forest_rural']"/>
	<remove xpath="/rwgmixer/district[@name='downtown']"/>
	<!--remove xpath="/rwgmixer/district[@name='gateway']"/-->

	<!--set xpath="/rwgmixer/township[@name='oldwest']/property[@name='spawn_gateway']/@value">true</set--> <!-- Stops roads appearing -->
	<append xpath="/rwgmixer/township[@name='oldwest']/property[@name='biomes']/@value">,forest,burntforest,snow,wasteland</append><!-- Expanding desert -->
	<set xpath="/rwgmixer/world[@name='tiny']/property[@class='oldwest']/property[@name='count']/@value">3, 4, 6</set>
	<set xpath="/rwgmixer/world[@name='tiny']/property[@class='oldwest']/property[@name='tiles']/@value">6, 12</set>
	<set xpath="/rwgmixer/world[@name='small']/property[@class='oldwest']/property[@name='count']/@value">4, 6, 8</set>
	<set xpath="/rwgmixer/world[@name='small']/property[@class='oldwest']/property[@name='tiles']/@value">6, 16</set>	
	<set xpath="/rwgmixer/world[@name='medium']/property[@class='oldwest']/property[@name='count']/@value">6, 9, 12</set>
	<set xpath="/rwgmixer/world[@name='medium']/property[@class='oldwest']/property[@name='tiles']/@value">6, 16</set>
	<set xpath="/rwgmixer/world[@name='large']/property[@class='oldwest']/property[@name='count']/@value">8, 12, 16</set>
	<set xpath="/rwgmixer/world[@name='large']/property[@class='oldwest']/property[@name='tiles']/@value">6, 16</set>
	<set xpath="/rwgmixer/world[@name='tiny']/property[@class='roadside']/property[@name='count']/@value">0, 0, 0</set>
	<set xpath="/rwgmixer/world[@name='small']/property[@class='roadside']/property[@name='count']/@value">0, 0, 0</set>
	<set xpath="/rwgmixer/world[@name='medium']/property[@class='roadside']/property[@name='count']/@value">0, 0, 0</set>
	<set xpath="/rwgmixer/world[@name='large']/property[@class='roadside']/property[@name='count']/@value">0, 0, 0</set>
	
	<append xpath="/rwgmixer">
	
	<streettile name="rwg_tile_oldwest_corner_02">
		<property name="maxtiles" value="1"/>
		<property name="mintiles" value="1"/>
		<property name="maxdensity" value="120"/>
	</streettile>
	
	<streettile name="rwg_tile_oldwest_straight_02">
		<property name="maxtiles" value="1"/>
		<property name="mintiles" value="1"/>
		<property name="maxdensity" value="120"/>
	</streettile>

	</append>	

</wildwestmod>